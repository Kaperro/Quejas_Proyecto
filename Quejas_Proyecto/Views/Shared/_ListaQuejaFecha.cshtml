@model Quejas_Proyecto.context.queja
<table class="table">
    <thead>
        <tr>
            <th >Queja</th>
            <th >Fecha</th>
            <th>Comercio</th>
        </tr>
    </thead>

    @foreach (var queja in ViewBag.ListaQueja)
    {

        <tbody>

            <tr>

                <td>@queja.queja1</td>
                <td>@queja.fecha_queja</td>
                <td>@queja.sucursal.comercio.nombre_comercio</td>
            </tr>

        </tbody>


    }
</table>