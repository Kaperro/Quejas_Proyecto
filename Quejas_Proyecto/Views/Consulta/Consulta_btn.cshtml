
@model IEnumerable<Quejas_Proyecto.context.Consulta>
@{
    ViewBag.Title = "Consulta";
}

<h2>@ViewBag.Title</h2>

<table class="responsive-table  striped">
    <tr>
        <th>
            CONTEO DE QUEJAS
        </th>

        <th>
            COMERCIO
        </th>
        <td>
            ID
        </td>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Queja
            </td>

            <td>
                @item.Comercio
            </td>
            <td>
                @item.Id_comercio
            </td>

        </tr>
    }


</table>
